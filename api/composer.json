{
    "name": "fcomarcet1/symfony-api-platform",
    "description": "Api rest symfony 5.1 && api-platform",
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.4",
        "ext-ctype": "*",
        "ext-fileinfo": "*",
        "ext-iconv": "*",
        "api-platform/core": "^2.5",
        "composer/package-versions-deprecated": "1.11.99.2",
        "doctrine/annotations": "^1.0",
        "doctrine/doctrine-bundle": "^2.3",
        "doctrine/doctrine-migrations-bundle": "^3.1",
        "doctrine/orm": "^2.9",
        "nelmio/cors-bundle": "^2.1",
        "phpdocumentor/reflection-docblock": "^5.2",
        "phpunit/phpunit": "^9.5",
        "symfony/asset": "5.1.*",
        "symfony/console": "5.1.*",
        "symfony/css-selector": "5.1.*",
        "symfony/dotenv": "5.1.*",
        "symfony/expression-language": "5.1.*",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "5.1.*",
        "symfony/monolog-bundle": "^3.7",
        "symfony/phpunit-bridge": "5.1.*",
        "symfony/property-access": "5.1.*",
        "symfony/property-info": "5.1.*",
        "symfony/proxy-manager-bridge": "5.1.*",
        "symfony/security-bundle": "5.1.*",
        "symfony/security-core": "5.1.*",
        "symfony/serializer": "5.1.*",
        "symfony/twig-bundle": "^5.1",
        "symfony/uid": "5.1.*",
        "symfony/validator": "5.1.*",
        "symfony/yaml": "5.1.*"
    },
    "require-dev": {
        "ekino/phpstan-banned-code": "^0.4.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan": "^0.12.93",
        "phpstan/phpstan-doctrine": "^0.12.42",
        "phpstan/phpstan-symfony": "^0.12.41",
        "symfony/browser-kit": "5.1.*",
        "symfony/debug-bundle": "5.1.*",
        "symfony/maker-bundle": "^1.33",
        "symfony/stopwatch": "5.1.*",
        "symfony/var-dumper": "5.1.*",
        "symfony/web-profiler-bundle": "5.1.*",
        "symplify/easy-coding-standard": "^9.4"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "cs": [
            "vendor/bin/ecs check --ansi --no-interaction"
        ],
        "cs-fix": [
            "vendor/bin/ecs check --fix --ansi --no-interaction"
        ],
        "phpstan": [
            "vendor/bin/phpstan analyse --ansi --no-interaction"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.1.*"
        }
    }
}
